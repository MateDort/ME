{
  "name": "meos",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run next\" \"npm run electron\"",
    "next": "next dev",
    "electron": "wait-on http://localhost:3000 && electron .",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:build": "next build && electron-builder",
    "build:electron": "next build && electron-builder --publish=never"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.2.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.6.0",
    "framer-motion": "^10.16.0",
    "next": "^14.0.0",
    "openai": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-syntax-highlighter": "^15.5.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "wait-on": "^9.0.3"
  }
}
